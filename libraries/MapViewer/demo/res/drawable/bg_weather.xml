    </item>
    <item android:maxLevel="9">
        <shape android:shape="oval">
            <solid android:color="@color/lightSnow" />
        </shape>
    </item>
    <item android:maxLevel="10">
        <shape android:shape="oval">
            <solid android:color="@color/heavySnow" />
        </shape>
    </item>
    <item android:maxLevel="11">
        <shape android:shape="oval">
            <solid android:color="@color/storm" />
        </shape>
    </item>
</level-list>